exports.AddWindow=function(){var e=(require("db"),Ti.UI.createWindow({modal:!0,title:"Add Item",backgroundColor:"#fff"})),a=Ti.UI.createScrollView({contentWidth:"auto",contentHeight:"auto",showVerticalScrollIndicator:!0,showHorizontalScrollIndicator:!0,height:Titanium.UI.FILL,width:Titanium.UI.FILL}),t=Ti.UI.createTextField({width:"300dp",height:"45dp",top:"70dp",hintText:"New Item",borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,returnKeyType:Titanium.UI.RETURNKEY_NEXT}),o=Ti.UI.createTextField({width:"300dp",height:"45dp",top:"135dp",hintText:"Category",borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,returnKeyType:Titanium.UI.RETURNKEY_NEXT}),i=Ti.UI.createTextField({width:"300dp",height:"45dp",top:"200dp",hintText:"Estimated Hours",borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,keyboardType:Titanium.UI.KEYBOARD_NUMBERS_PUNCTUATION,returnKeyType:Titanium.UI.RETURNKEY_NEXT}),r=Ti.UI.createTextField({width:"300dp",height:"45dp",top:"265dp",hintText:"Due Date",borderStyle:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,keyboardType:Titanium.UI.KEYBOARD_NUMBERS_PUNCTUATION,returnKeyType:Titanium.UI.RETURNKEY_DONE});t.addEventListener("return",function(){o.focus()}),o.addEventListener("return",function(){i.focus()}),i.addEventListener("return",function(){r.focus()}),r.addEventListener("return",function(){addTask(t.value,o.value,i.value,r.value,e)});var l=Ti.UI.createButton({title:"Add",width:"300dp",height:"40dp",bottom:"85dp"});l.addEventListener("click",function(){addTask(t.value,o.value,i.value,r.value,e)});var d=Ti.UI.createButton({title:"Cancel",width:"300dp",height:"40dp",bottom:"40dp"});return d.addEventListener("click",function(){e.close()}),a.add(t),a.add(o),a.add(i),a.add(r),a.add(l),a.add(d),e.add(a),e};var addTask=function(e,a,t,o,i){return""===e||""===a||""===t||""===o?void alert("Please provide more details about this task"):(require("db").addItem(e,a,t,o),Ti.App.fireEvent("app:updateTables"),void i.close())};