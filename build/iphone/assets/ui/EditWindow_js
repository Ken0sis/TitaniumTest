exports.EditWindow=function(e,a,t){Ti.API.info(e);var o=require("db"),i=(o.selectByID(e),o.getTotalRewards(e)),r={};r.reward1="images/Numbers-1-filled-icon.png",r.reward2="images/Numbers-2-filled-icon.png",r.reward3="images/Numbers-3-filled-icon.png",r.reward4="images/Numbers-4-filled-icon.png",r.reward5="images/Numbers-5-filled-icon.png",r.reward6="images/Numbers-6-filled-icon.png",r.reward7="images/Numbers-7-filled-icon.png";var d=Ti.UI.createWindow({title:"Edit Item",backgroundColor:"#FFFFFF",barColor:"#FFFFFF",translucent:!1,tabBarHidden:!0}),l=Titanium.UI.createTabGroup(),n=Titanium.UI.createTab({title:"Edit Item",window:d}),m=Ti.UI.createView({width:Titanium.UI.SIZE,horizontalWrap:"true",layout:"horizontal"}),s=Ti.UI.createView({width:.5,borderWidth:.25,borderColor:"#ADADAD",top:20,height:150}),u=Ti.UI.createView({left:"50%"}),T=Ti.UI.createLabel({width:Titanium.UI.SIZE,height:Titanium.UI.SIZE,top:25,text:0==o.selectByID(e)[0].goalGuide?"F":o.selectByID(e)[0].goalGuide,textAlign:"center",color:"#0080f0",font:{fontSize:60}});Ti.App.addEventListener("app:updateDisplay",function(){T.text=0==o.selectByID(e)[0].goalGuide?"-":o.selectByID(e)[0].goalGuide});var f=(Ti.UI.createLabel({width:25,height:25,bottom:120,backgroundImage:r.reward1}),Ti.UI.createLabel({text:"$"+i,width:100,layout:"horizontal",textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT,font:{fontSize:11}}));Ti.App.addEventListener("app:updateDisplay",function(){f.text="$"+o.getTotalRewards(e)});var c=Ti.UI.createButton({title:"Done",font:{fontFamily:"HelveticaNeue"}});c.addEventListener("click",function(){Ti.App.fireEvent("app:updateTables"),l.close()});var y=Ti.UI.createButton({title:"+ Work",width:"35%",height:43,bottom:25,backgroundColor:"#0080f0",color:"white",borderRadius:7,font:{fontFamily:"HelveticaNeue",fontSize:19}});return y.addEventListener("touchstart",function(){o.addWork(e,t),Ti.App.fireEvent("app:updateDisplay")}),m.add(f),d.setLeftNavButton(m),d.add(s),d.add(u),d.add(y),d.setRightNavButton(c),u.add(T),l